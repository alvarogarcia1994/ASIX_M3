Algoritmo sin_titulo
	
	//bloque de variables
	
	Definir a Como Real;
	Definir b Como Real;
	Definir aux Como Real;
	
	//entrada de datos
	
	Escribir "define el valor de a: ";
	Leer a;
	Escribir "define el valor de b: ";
	Leer b;
	
	//calculos
	
	aux <- a;
	a <- b;
	b <- aux;
	
	//salida de datos
	
	Escribir "a es ", a;
	Escribir "b es ", b;
FinAlgoritmo
